<p id="notice"><%= notice %></p>

<h1>Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Upload</th>
      <th>Input</th>
      <th>Account string</th>
      <th>Status</th>
      <th>D9</th>
      <th>D8</th>
      <th>D7</th>
      <th>D6</th>
      <th>D5</th>
      <th>D4</th>
      <th>D3</th>
      <th>D2</th>
      <th>D1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.upload_id %></td>
        <td><%= account.input %></td>
        <td><%= account.account_string %></td>
        <td><%= account.status %></td>
        <td><%= account.d9 %></td>
        <td><%= account.d8 %></td>
        <td><%= account.d7 %></td>
        <td><%= account.d6 %></td>
        <td><%= account.d5 %></td>
        <td><%= account.d4 %></td>
        <td><%= account.d3 %></td>
        <td><%= account.d2 %></td>
        <td><%= account.d1 %></td>
        <td><%= link_to 'Show', account %></td>
        <td><%= link_to 'Edit', edit_account_path(account) %></td>
        <td><%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account', new_account_path %>
