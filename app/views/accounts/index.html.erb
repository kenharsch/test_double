<p id="notice"><%= notice %></p>

<h1>Accounts</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Upload</th>
      <th scope="col">Account string</th>
      <th scope="col">Status</th>
      <th scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.upload_id %></td>
        <td><%= account.account_string %></td>
        <td><%= account.status %></td>
        <td><%= link_to 'Show', account %> | <%= link_to 'Edit', edit_account_path(account) %> | <%= link_to 'Destroy', account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account', new_account_path %>
